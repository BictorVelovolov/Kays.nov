#include <iostream>
#include <vector>
#include <fstream>
#include <string>
#include "sortic.h"

using namespace std;

int main()
{
    int n, zn, mini, kon;
    ofstream file;
    string fin;
    cout << "kol-vo elementov v 1-om: ";
    cin >> n;
    kon = 0;
    mini =  2147483647;
    vector <int> a (n, 0);
    vector <int> b;
    file.open("aboba.txt");
    for (int i = 0; i < n; i++){
        cin >> zn;
        a[i] = zn;
    }
    while (kon < a.size()){
            kon = 0;
           mini =  2147483647;
        for (int i = 0; i < a.size(); i++){
        if (a[i] <= mini)
            mini = a[i];
    }
    while (a[0] != mini){
            //if(a[1] != mini){
               r(a);
                file << "ra" << endl;
            //}
            //else{
            //    s(a);
           // file << "sa" << endl;
          //  }
        }
        p(b, a);
        file << "pb" << endl;
        a[0] = 2147483647;
        for (int l = 0; l < a.size(); l++)
            if (a[l] == 2147483647){
                kon++;
            }


    }
    file.close();
    /*for (int i = 0; i < b.size(); i++)
        cout << b[i] << " ";*/
    return 0;
}
