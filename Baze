#include <iostream>
#include <vector>
#include <fstream>
#include <string>
#include "sortic.h"

using namespace std;

int main()
{
    int n, zn, mini, kon;
    ofstream file;
    string fin, znak;
    cin >> znak;
    kon = 0;
    vector <int> a;
    vector <int> a1;
    vector <int> b;
    file.open("aboba.txt");
    for(int i = 0; znak != "!"; i++){
        a.push_back(cifrovizacia(znak));
        cin >> znak;
    }
    while (kon < a.size()){
            kon = 0;
           mini = minimum(a);
    while (a[0] != mini){
               r(a);
                file << "ra" << endl;
                cout << "ra" << endl;
    }
        p(b, a);
        file << "pb" << endl;
        cout << "pb" << endl;
        for (int l = 0; l < a.size(); l++)
            if (a[l] == 2147483647)
                kon++;
    }
    for (int i = 0; i < b.size(); i++){
        rr(b);
        file << "rrb" << endl;
        cout << "rrb" << endl;
        p(a1, b);
        file << "pa" << endl;
        cout << "pa" << endl;
    }
    zamen(a1, a);
    for (int i = 0; i < a.size(); i++)
        cout << a[i] << " ";
    file.close();
    return 0;
}
