#include <iostream>
#include <vector>
#include <fstream>
#include <string>
#include "sortic.h"

using namespace std;

int main()
{
    int n, zn, mini;
    ofstream file;
    cout << "kol-vo elementov v 1-om: ";
    cin >> n;
    mini =  2147483647;
    vector <int> a (n, 0);
    vector <int> b (0, 0);
    for (int i = 0; i < n; i++){
        cin >> zn;
        a[i] = zn;
    }
    for (int i = 0; i < a.size(); i++){
        if (a[i] < mini)
            mini = a[i];
    }
    while (a[0] != mini){
            if(a[1] != maxi){
               r(a);
                cout << "ra";
            }
            else
            s(a);
            cout << "sa";
        }
        p(b, a);
        cout << "pb";
}
